{
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#e46876",
	  "background": "#181616",
          "properties": {
            "folder_icon": "",
            "folder_separator_icon": "",
            "home_icon": "",
            "max_depth": 0,
            "style": "agnoster_short"
          },
          "style": "powerline",
          "template": "{{ .Path }}",
          "type": "path"
        },
        {
          "background": "#181616",
          "foreground": "#8993a7",
          "properties": {
            "branch_max_length": 25,
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " ({{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }})",
          "type": "git"
        },
        {
          "background": "#181616",
          "foreground": "#e46876",
          "properties": {
            "always_enabled": true
          },
          "style": "powerline",
          "template": " -> {{ .FormattedMs }}",
          "type": "executiontime"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "#e4d794",
          "foreground": "#E4F34A",
          "leading_diamond": " \ue0b6",
          "properties": {
            "fetch_version": false
          },
          "style": "diamond",
          "template": "\ue235{{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}",
          "trailing_diamond": "\ue0b4",
          "type": "python"
        },
        {
          "background": "#e4d794",
          "backgroundbak": "#-4c1f5e",
          "foreground": "#7FD5EA",
          "foregroundbak": "#-7FD5EA",
          "leading_diamond": " \ue0b6",
          "properties": {
            "fetch_version": false
          },
          "style": "diamond",
          "template": "\ue626{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "trailing_diamond": "\ue0b4",
          "type": "go"
        },
        {
          "background": "#e4d794",
          "backgroundbak": "#-4c1f5e",
          "foreground": "#42E66C",
          "foregroundbak": "#-42E66C",
          "leading_diamond": " \ue0b6",
          "properties": {
            "fetch_version": false
          },
          "style": "diamond",
          "template": "\ue718{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}",
          "trailing_diamond": "\ue0b4",
          "type": "node"
        },
        {
          "background": "#e4d794",
          "backgroundbak": "#-4c1f5e",
          "foreground": "#E64747",
          "foregroundbak": "#-E64747",
          "leading_diamond": " \ue0b6",
          "properties": {
            "fetch_version": false
          },
          "style": "diamond",
          "template": "\ue791{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "trailing_diamond": "\ue0b4",
          "type": "ruby"
        },
        {
          "background": "#e4d794",
          "foreground": "#E64747",
          "leading_diamond": " \ue0b6",
          "properties": {
            "fetch_version": false
          },
          "style": "diamond",
          "template": "\ue738{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "trailing_diamond": "\ue0b4",
          "type": "java"
        }
      ],
      "type": "rprompt"
    },
    {
      "alignment": "left",
      "newline": false,
      "segments": [
        {
          "background": "#181616",
          "foreground": "#d9a594",
          "style": "diamond",
          "template": ":",
          "type": "time"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{ .Shell }} - {{ .Folder }}",
  "final_space": true,
  "version": 2
}
